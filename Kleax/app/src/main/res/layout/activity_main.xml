            android:background="@drawable/enanao_sesion_inicio" />

        <TextView
            android:id="@+id/kleax"
            android:layout_width="141dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginTop="63dp"
            android:gravity="center_horizontal|top"
            android:text="@string/kleax"
            android:textAppearance="@style/kleax" />

        <TextView
            android:id="@+id/compra_hoy_"
            android:layout_width="342dp"
            android:layout_height="28dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="22dp"
            android:layout_marginTop="109dp"
            android:gravity="top"
            android:text="@string/compra_hoy_"
            android:textAppearance="@style/compra_hoy_" />

        <TextView
            android:id="@+id/reg_strese"
            android:layout_width="131dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="166dp"
            android:layout_marginTop="596dp"
