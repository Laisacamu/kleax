<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/fondo_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#F5F5DC"
        android:clipToOutline="true"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="-16dp">

        <View
            android:id="@+id/rectangle_fo"
            android:layout_width="119dp"
            android:layout_height="97dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-53dp"
            android:layout_marginTop="266dp"
            android:background="@drawable/rectangle_fo" />

        <View
            android:id="@+id/rectangle_fo2"
            android:layout_width="342dp"
            android:layout_height="119dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-224dp"
            android:layout_marginTop="334dp"
            android:background="@drawable/rectangle_fo" />

        <View
            android:id="@+id/rectangle_fo3"
            android:layout_width="335dp"
            android:layout_height="125dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="269dp"
            android:layout_marginTop="304dp"
            android:layout_marginEnd="-320dp"
            android:background="@drawable/rectangle_fo" />

        <View
            android:id="@+id/rectangle_fo4"
            android:layout_width="399dp"
            android:layout_height="127dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="217dp"
            android:layout_marginTop="382dp"
            android:layout_marginEnd="-350dp"
            android:background="@drawable/rectangle_fo" />

        <View
            android:id="@+id/bolsas_de_c"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="14dp"
            android:layout_marginTop="304dp"
            android:background="@drawable/bolsas_de_compras" />

        <View
            android:id="@+id/compras_mov"
            android:layout_width="130dp"
            android:layout_height="160dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="243dp"
            android:layout_marginTop="303dp"
