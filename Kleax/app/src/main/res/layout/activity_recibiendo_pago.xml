<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".recibiendo_pago">

    <RelativeLayout
        android:id="@+id/recibiendo_"
        android:layout_width="438dp"
        android:layout_height="778dp"
        android:background="#88A7E9"
        android:clipToOutline="true"
        android:elevation="4dp"
        tools:ignore="MissingConstraints">

        <View
            android:id="@+id/rectangle_1"
            android:layout_width="279dp"
            android:layout_height="287dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="72dp"
            android:layout_marginTop="249dp"
            android:background="@drawable/rectangle_1"
            android:elevation="1dp" />

        <TextView
            android:id="@+id/kleax_rp"
            android:layout_width="231dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="87dp"
            android:layout_marginTop="39dp"
            android:gravity="center_horizontal|top"
            android:text="@string/kleax"
            android:textAppearance="@style/kleax_rp" />

        <View
            android:id="@+id/spinner_de_"
            android:layout_width="68dp"
            android:layout_height="71dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="182dp"
            android:layout_marginTop="338dp"
            android:background="@drawable/recibido"
            android:elevation="3dp" />

        <TextView
            android:id="@+id/recibiendo_pago"
            android:layout_width="187dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="127dp"
            android:layout_marginTop="261dp"
            android:elevation="3dp"
            android:gravity="center_vertical"
            android:text="@string/pago_recibido"
            android:textAppearance="@style/pago_recibido" />

        <View
            android:id="@+id/some_id"
            android:layout_width="110dp"
            android:layout_height="106dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="56dp"
            android:layout_marginTop="436dp"
            android:background="@drawable/pollo"
            android:elevation="3dp" />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>